<%= simple_form_for(@survivor) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :residence %>
    <%= f.input :photo_url %>
    <%= f.input :fantasy %>
    <%= f.input :elimination_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
